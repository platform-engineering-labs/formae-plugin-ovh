module keypair

import "@formae/formae.pkl"
import "../ovh.pkl"

const type = "OVH::Compute::Keypair"

@ovh.ResourceHint {
  type = module.type
  identifier = "name"
}
open class Keypair extends formae.Resource {
  @ovh.FieldHint {
    required = true
    createOnly = true
  }
  name: String

  @ovh.FieldHint {
    createOnly = true
  }
  publicKey: String?

  // fingerprint is computed by OpenStack - not user-provided
}
